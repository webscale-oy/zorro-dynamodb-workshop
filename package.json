  },
  "devDependencies": {
    "@types/chance": "^1.0.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.3",
    "@types/supertest": "^2.0.6",
    "@types/uuid": "^3.4.4",
    "chance": "^1.0.16",
    "jest": "^23.6.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.30",
    "typescript": "^3.1.3"
