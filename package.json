{
  "name": "photo-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "test": "jest --runInBand",
    "build-ts": "tsc",
    "dev": "ts-node-dev src/server.ts --ignore-watch node_modules"
  },
  "author": "hp@webscale.fi",
  "license": "ISC",
  "dependencies": {
